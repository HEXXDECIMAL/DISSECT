// Migrated from malcontent: credential/shell/zsh_history.yara

rule zsh_history: high {
  meta:
    description = "accesses zsh shell history"
    confidence  = "0.66"

  strings:
$ref              = ".zsh_history" fullword
    $not_appsec_rules = "\"id\": \"crs-930-120\""
  condition:
    $ref and none of ($not*)
}

rule zsh_history_editor: override {
  meta:
    description = "editor"
    confidence  = "0.66"
    zsh_history = "medium"

  strings:
$ref = ".zsh_history" fullword

    $not_VIMRUNTIME = "VIMRUNTIME"
  condition:
    $ref and any of ($not*)
}
